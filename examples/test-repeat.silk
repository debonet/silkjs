<!-- repeat element -->
<defelt scope="_" name="repeat" times="0">
	var vscope = [];
	var vlv    = [];

	return function(){
		var jq = $();
		var n;
		var c = _.times;
		var cAlloc = vscope.length;

		// delete exess
		vscope = vscope.slice(0,c);
		vlv    = vlv.slice(0,c);

		// make new ones, if needed
		for (n=vscope.length; n<c; n++){
			vscope[n] = _.clone();
			vscope[n].defvar("_index",n);
			vlv[n] = new LiveValue("x",_._createInner(vscope[n]));
		}

		// eval and add
		for (n=0; n<c; n++){
			jq = jq.add(vlv[n].fxGet());
		}
		return jq;
	};
</defelt>


