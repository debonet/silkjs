
<defmacro name="studentrow" student="'some body'" grade="'n/a'">
	<tr>
		<td>
			{{_.student}}
		</td>
		<td>
			{{_.grade}}
		</td>
	</tr>
</defmacro>

<table>
	<studentrow student="'Bob Smith'" grade="'A+'" />
	<studentrow student="'John Doe'" grade="'B-'" />
</table>




